<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" 
	version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee">

<display-name>NGWMN Map Client (formerly GWDP MAP){Owner: NB, JL Coder: IK}</display-name>
<description>Client Application of the NGWMN (formerly GWDP)</description>

	<filter>
		<display-name>DownloadTokenFilter</display-name>
		<filter-name>DownloadTokenFilter</filter-name>
		<filter-class>gov.usgs.DownloadTokenFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>DownloadTokenFilter</filter-name>
		<servlet-name>ExportServlet</servlet-name>
	</filter-mapping>

	<servlet>
		<description>Proxy to forward requests to the Oracle MapViewer server</description>
		<servlet-name>MVServlet</servlet-name>
		<servlet-class>gov.usgs.MVServlet</servlet-class>
	</servlet>

	<servlet>
		<description>Process IDENTIFY map click spatially</description>
		<servlet-name>IdentifyServlet</servlet-name>
		<servlet-class>gov.usgs.IdentifyServlet</servlet-class>
	</servlet>
	
	<servlet>
		<description>Process BOUNDING BOX action on map</description>
			<servlet-name>BBoxServlet</servlet-name>
			<servlet-class>gov.usgs.BBoxServlet</servlet-class>
	</servlet>
	
	<servlet>
		<description>Retrieve site summary data</description>
			<servlet-name>IdentifyDataProxy</servlet-name>
			<servlet-class>gov.usgs.IdentifyDataProxy</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ExportServlet</servlet-name>
		<servlet-class>gov.usgs.ExportServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>DiagnoseServlet</servlet-name>
		<servlet-class>gov.usgs.DiagnoseServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SettingsServlet</servlet-name>
		<servlet-class>gov.usgs.SettingsServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ContactService</display-name>
		<servlet-name>ContactService</servlet-name>
		<servlet-class>gov.usgs.ContactService</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>MVServlet</servlet-name>
  		<url-pattern>/mvwms</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>IdentifyServlet</servlet-name>
  		<url-pattern>/identify</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>BBoxServlet</servlet-name>
  		<url-pattern>/bbox</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>IdentifyDataProxy</servlet-name>
  		<url-pattern>/iddata</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ExportServlet</servlet-name>
  		<url-pattern>/export</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>DiagnoseServlet</servlet-name>
		<url-pattern>/diagnose</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SettingsServlet</servlet-name>
		<url-pattern>/settings</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ContactService</servlet-name>
		<url-pattern>/feedback/submit</url-pattern>
	</servlet-mapping>
		
	<session-config>
 		<session-timeout>30</session-timeout>
 	</session-config>

</web-app>
